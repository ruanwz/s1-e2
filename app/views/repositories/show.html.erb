<p id="notice"><%= notice %></p>

<p>
  <b>Name:</b>
  <%= @repository.name %>
</p>

<p>
  <b>User:</b>
  <%= @repository.user.username %>
</p>

<p>
  <b>Type:</b>
  <%= @repository.repository_type.name %>
</p>

<p>
  <b>Fork by:</b>
  <%= @repository.fork_from ? @repository.fork_from.name : 'na' %>
</p>

<p>
  <b>Created at:</b>
  <%= @repository.created_at %>
</p>


<%= link_to 'Edit', edit_repository_path(@repository) %> |
<%= link_to 'Back', repositories_path %>
