<h1>Listing commit_notes</h1>

<table>
  <tr>
    <th>Note</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @commit_notes.each do |commit_note| %>
  <tr>
    <td><%= commit_note.note %></td>
    <td><%= link_to 'Show', commit_note %></td>
    <td><%= link_to 'Edit', edit_commit_note_path(commit_note) %></td>
    <td><%= link_to 'Destroy', commit_note, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Commit note', new_commit_note_path %>
